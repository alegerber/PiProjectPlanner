{% extends "04-templates/base.html.twig" %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('css/tab-view') }}
{% endblock %}
{% block title %}Component{% endblock %}
{% block content %}
    <div class="container mt-3">
        <h1>{{ component.title }}</h1>
        {% include '04-templates/component-view-tab.html.twig' with {'component': component } %}
    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('js/tab-form-edit') }}
{% endblock %}